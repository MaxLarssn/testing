{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "typecheck": "tsc",
    "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
    "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "@expo-google-fonts/delius-unicase": "^0.2.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^7.1.9",
    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "aws-amplify": "^4.3.14",
    "aws-amplify-react-native": "^6.0.2",
    "aws-appsync": "^4.0.1",
    "cross-fetch": "^3.1.4",
    "expo": "~44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-av": "~10.2.0",
    "expo-font": "~10.0.4",
    "expo-haptics": "~11.1.0",
    "expo-linear-gradient": "~11.0.0",
    "expo-status-bar": "~1.2.0",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.11.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "^0.64.24",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
